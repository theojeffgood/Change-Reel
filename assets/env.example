# NextAuth
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=replace_with_strong_secret

# GitHub App OAuth (identity only)
OAUTH_CLIENT_ID=replace_me
OAUTH_CLIENT_SECRET=replace_me

# GitHub App credentials
GITHUB_APP_ID=replace_me
# Private key: either a single-line with \n escapes or multiline in your secret manager
GITHUB_APP_PRIVATE_KEY=-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----\n
GITHUB_APP_WEBHOOK_SECRET=replace_me

# OpenAI API configuration
OPENAI_API_KEY=sk-replace_me
OPENAI_MODEL=gpt-4o



